@article{gullahorn_computer_1965-1,
	title = {Some {Computer} {Applications} in {Social} {Science}},
	volume = {30},
	issn = {00031224},
	url = {http://www.jstor.org/stable/2090716?origin=crossref},
	doi = {10.2307/2090716},
	abstract = {The general symbol manipulating capacities of the modern digital computer offer a valuable tool for theory development and verification. Diverse applications of computer models in social science are discussed, along with a validation study involving HOMUNCULUS, a computer model of social behavior. Hypotheses concerning decision making in a role-conflict dilemma were stated operationally as computer routines compatible with HOMUNCULUS. In computer runs using input data from actual questionnaires, alternative formulations regarding role conflict resolution were tested. Since the data generated by each run represented the logical consequences of the hypotheses operating in that run, it was possible to assess the adequacy of each programmed formulation by comparing the simulated responses with the actual decisions of living respondents. Such flexibility and dynamic capacity are major benefits accruing from testing a social theory through computer simulation.},
	number = {3},
	urldate = {2025-04-16},
