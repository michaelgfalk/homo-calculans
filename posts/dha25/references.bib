
@article{clarkson_simulation_1960,
	title = {{SIMULATION} {OF} {INDIVIDUAL} {AND} {GROUP} {BEHAVIOR}.},
	volume = {50},
	issn = {0002-8282},
	abstract = {Simulation is a technique for building theories that reproduce part or all of the output of a behaving system. The system can be an aggregate of behaving units, an entire economy, or a particular unit, a human decision-maker. The article, outlines some methods for using simulation to study various aspects of an economic system. The article places special emphasis on microeconomic simulation, especially the simulation of individual economic actors and individual firms. Simulation appears to offer new approaches to the aggregation problem. To the extent that whole classes of individual decision units in the economy share relatively invariant structures, aggregation can be attempted by writing programs for individual units, and treating these as representative units. One of its attractions lies in the new opportunities it affords for direct confrontation of the theory with concrete behavior. It does not restrict to viewing the economic system through the wrong end of a telescope-limiting people to census data and similar kinds of statistics.},
